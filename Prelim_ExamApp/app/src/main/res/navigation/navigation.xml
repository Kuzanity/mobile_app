        tools:layout="@layout/fragment_f_a_q" />
    <fragment
        android:id="@+id/sendInquiries"
        android:name="com.example.prelim_examapp.SendInquiries"
        android:label="INQUIRIES"
        tools:layout="@layout/fragment_send_inquiries" />
    <fragment
        android:id="@+id/newsFragment"
        android:name="com.example.prelim_examapp.NewsFragment"
        android:label="NEWS" />
    <activity
        android:id="@+id/mapsActivity2"
        android:name="com.example.prelim_examapp.MapsActivity"
        android:label="MAPS"
        tools:layout="@layout/activity_maps" />
