        tools:layout="@layout/fragment_home" >

    </fragment>
    <fragment
        android:id="@+id/about"
        android:name="com.example.prelim_examapp.About"
        android:label="ABOUT"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/content2"
        android:name="com.example.prelim_examapp.Content"
        android:label="CONTENT"
        tools:layout="@layout/fragment_content" />
    <fragment
        android:id="@+id/FAQ"
        android:name="com.example.prelim_examapp.FAQ"
