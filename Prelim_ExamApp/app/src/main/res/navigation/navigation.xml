<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/home2">
    <fragment
        android:id="@+id/login2"
        android:name="com.example.prelim_examapp.Login"
        android:label="LOGIN"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login2_to_registration"
            app:destination="@id/registration" />
        <action
            android:id="@+id/action_login2_to_home2"
            app:destination="@id/home2" />
    </fragment>
    <fragment
        android:id="@+id/registration"
        android:name="com.example.prelim_examapp.Registration"
        android:label="REGISTRATION"
        tools:layout="@layout/fragment_registration" />
    <fragment
        android:id="@+id/home2"
        android:name="com.example.prelim_examapp.Home"
